  "scripts": {
    "dev": "NODE_ENV=development tsx watch server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:anime-api": "esbuild api/anime/index.js --bundle --platform=node --format=esm --packages=external --outfile=api/anime/index.bundled.js",
    "build:netlify": "vite build && cp public/favicon.* dist/public/",
    "build:vercel": "rm -rf dist && npm run build:anime-api && vite build && cp public/favicon.* dist/public/ && cp -r api dist/",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "expo:start": "expo start",
    "expo:web": "expo start --web",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:build:android": "eas build --platform android",
    "expo:build:ios": "eas build --platform ios",
    "expo:build:web": "expo export --platform web",
    "expo:build:android-tv": "eas build --platform android --profile android-tv",
    "test:platforms": "node scripts/test-platforms.js",
    "test:vidsrc": "node scripts/test-vidsrc.cjs",
    "test:movix-download": "node scripts/test-movix-download.cjs",
    "test:movix-conversion": "node scripts/test-movix-conversion.cjs",
    "test:movix-simple": "node scripts/test-movix-simple.cjs",
    "test:supervideo": "node scripts/test-supervideo.cjs",
    "test:hls-flow": "node scripts/test-hls-flow.cjs",
    "test:movix-supervideo": "node scripts/test-movix-supervideo.cjs"
  },
