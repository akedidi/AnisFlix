{
  "functions": {
    "api/extract.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/movix-proxy/index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}