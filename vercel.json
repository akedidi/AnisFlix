{
  "functions": {
    "api/**/*": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/auth/trakt/callback",
      "destination": "/api/auth/trakt/callback"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}